}>\\r\\n                    Leave Type: \\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    <Tag color=\\\"volcano\\\" style={{width:'145px'}}>{leave.type}</Tag>\\r\\n                    </Col>\\r\\n                    <Col span={8}>\\r\\n                    Number Of Days: \\r\\n                    <Tag color=\\\"volcano\\\" style={{width:'145px'}}>{leave.number_of_leave_days}</Tag>\\r\\n                    </Col>\\r\\n                  </Row>\\r\\n                  <br/>\\r\\n                  <Row>\\r\\n                    <Col span={6}>\\r\\n                    Start Date: \\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                      <Tag color=\\\"volcano\\\" style={{width:'105px'}}>\\r\\n                        {leave.startDate} \\r\\n                      </Tag>\\r\\n                    </Col>\\r\\n                    <Col span={6}>\\r\\n                    Start Time: \\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    <Tag color=\\\"volcano\\\" style={{width:'105px'}}> \\r\\n                      {leave.startHalf}\\r\\n                    </Tag>\\r\\n                    </Col>\\r\\n                    <Col span={6}>\\r\\n                    End Date: \\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                      <Tag color=\\\"volcano\\\" style={{width:'105px'}}>\\r\\n                        {leave.endDate} \\r\\n                      </Tag>\\r\\n                    </Col>\\r\\n                    <Col span={6}>\\r\\n                    End Time: \\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    <Tag color=\\\"volcano\\\" style={{width:'105px'}}>\\r\\n                      {leave.endHalf}\\r\\n                    </Tag>\\r\\n                    </Col>\\r\\n                  </Row>\\r\\n                  <br/>\\r\\n                  <Row>\\r\\n                    <Col span={12}>\\r\\n                    Supervisor 01: \\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                      <Tag color=\\\"volcano\\\" style={{width:'225px'}}>{leave.supervisor1}</Tag>\\r\\n                    </Col>\\r\\n                    <Col span={12}>\\r\\n                    Supervisor 02:\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    <Tag color=\\\"volcano\\\" style={{width:'225px'}}>{leave.supervisor2}</Tag>\\r\\n                    </Col>\\r\\n                  </Row>\\r\\n                  <br/>\\r\\n                  <Row>\\r\\n                    <Col span={24}>\\r\\n                    Special Note: \\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    &nbsp;&nbsp;&nbsp;\\r\\n                    <Tag color=\\\"volcano\\\" style={{width:'460px'}}>{leave.specialNotes}</Tag>\\r\\n                    </Col>\\r\\n                  </Row>\\r\\n                  <br/>\\r\\n                  </div>\\r\\n                </Modal> : null}\\r\\n            </div>\\r\\n        )\\r\\n    }\\r\\n}    \\r\\n\\r\\n\\r\\nconst WrappedPendingLeaves = Form.create({ name: 'pendingLeaves' })(PendingLeaves);\\r\\n\\r\\nexport default WrappedPendingLeaves;\",\"\\r\\nimport React from 'react';\\r\\nimport axios from '../../config/axios'\\r\\nimport moment from 'moment';\\r\\nimport 'antd/dist/antd.css';\\r\\nimport './index.css';\\r\\nimport {\\r\\n  Form,\\r\\n  Input,\\r\\n  Tooltip,\\r\\n  Icon,\\r\\n  Select,\\r\\n  Button,\\r\\n  DatePicker,\\r\\n  Radio,\\r\\n  Card,\\r\\n  Row,\\r\\n  Col,\\r\\n  message,\\r\\n  InputNumber,\\r\\n  Upload\\r\\n} from 'antd';\\r\\n\\r\\nconst { Option } = Select;\\r\\n\\r\\nfunction getBase64(img, callback) {\\