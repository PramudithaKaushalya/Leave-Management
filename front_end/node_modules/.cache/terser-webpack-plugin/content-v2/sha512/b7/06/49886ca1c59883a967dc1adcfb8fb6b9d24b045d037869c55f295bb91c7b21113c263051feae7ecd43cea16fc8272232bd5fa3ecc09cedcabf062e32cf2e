  this.setState({ searchText: '' });\\r\\n      };   \\r\\n    \\r\\n      showAddDrawer = () => {\\r\\n        this.setState({\\r\\n            visibleAdd: true\\r\\n          });\\r\\n      }\\r\\n\\r\\n      onAddClose = () => {\\r\\n          this.setState({\\r\\n            visibleAdd: false\\r\\n          });\\r\\n          this.reload();\\r\\n      };\\r\\n\\r\\n      showUpdateDrawer = (emp) => {\\r\\n    \\r\\n        axios.get('http://localhost:5000/user/get/'+ emp.id, \\r\\n        {\\r\\n            headers: {\\r\\n                Authorization: 'Bearer ' + localStorage.getItem(\\\"header\\\")\\r\\n            }\\r\\n        })\\r\\n        .then(res => {\\r\\n            this.setState({\\r\\n                visibleUpdate: true,\\r\\n                updateEmployee : res.data\\r\\n            })\\r\\n        })\\r\\n      };\\r\\n    \\r\\n    onUpdateClose = () => {\\r\\n        this.setState({\\r\\n          visibleUpdate: false,\\r\\n          updateEmployee: []\\r\\n        });\\r\\n        window.location.reload();\\r\\n    };\\r\\n    \\r\\n    showModal = (emp) => {\\r\\n      this.setState({ \\r\\n        visibleResign : true,\\r\\n        emp_id : emp.id\\r\\n      });\\r\\n    };\\r\\n\\r\\n    saveFormRef = formRef => {\\r\\n      this.formRef = formRef;\\r\\n    };\\r\\n\\r\\n    handleResign = () => {\\r\\n      const { form } = this.formRef.props;\\r\\n      form.validateFields((err, values) => {\\r\\n        console.log(values)\\r\\n        if (!err) {\\r\\n          axios.post('http://localhost:5000/user/resign', \\r\\n                {\\r\\n                  id: this.state.emp_id,\\r\\n                  resignDate:values.date\\r\\n                },\\r\\n                {\\r\\n                    headers: {\\r\\n                        Authorization: 'Bearer ' + localStorage.getItem(\\\"header\\\")\\r\\n                    }\\r\\n                })\\r\\n                .then(res => {\\r\\n                    message.success(res.data); \\r